{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Limits": {
      "KeepAliveTimeout": "00:02:10",
      "RequestHeadersTimeout": "00:00:30",
      "MaxRequestBodySize": 5242880
    },
    "Endpoints": {
      "Http": {
        "Url": "http://+:8080",
        "Protocols": "Http1AndHttp2"
      },
      "Grpc": {
        "Url": "http://+:8081",
        "Protocols": "Http2"
      }
    }
  },
  "AllowedHosts": "*"
}
